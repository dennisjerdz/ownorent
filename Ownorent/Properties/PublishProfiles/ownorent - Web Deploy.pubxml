            <Source Path="DBMigration" DbContext="Ownorent.Models.ApplicationDbContext, Ownorent" MigrationConfiguration="Ownorent.Migrations.Configuration, Ownorent" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
